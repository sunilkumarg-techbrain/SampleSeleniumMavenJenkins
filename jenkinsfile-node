node {
	stage ('GIT checkout'){
		git "https://github.com/sunilkumarg-techbrain/SampleSeleniumMavenJenkins"
		}
	stage ('Maven Test Execution'){
      dir("SampleSeleniumMavenJenkins") {
	   sh "mvn clean install"
       }   
	stage ('Allure Report Generation'){
       dir("SampleSeleniumMavenJenkins") {
	   sh "mvn allure:report"
       }
		}
}
